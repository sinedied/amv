{
  "name": "amv",
  "version": "1.0.0",
  "description": "Quick AI Renamer - AI-powered bulk file and folder renaming tool",
  "type": "module",
  "main": "dist/cli/index.js",
  "bin": {
    "amv": "bin/amv.js"
  },
  "scripts": {
    "build": "vite build && cp dist/web/app.js public/app.js",
    "dev": "vite build --watch",
    "start": "node bin/amv.js",
    "type-check": "tsc --noEmit",
    "postbuild": "cp dist/web/app.js public/app.js"
  },
  "keywords": [
    "ai",
    "rename",
    "files",
    "cli",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/static": "^8.2.0",
    "commander": "^14.0.0",
    "fastify": "^5.3.3",
    "lit": "^3.2.1",
    "openai": "^5.3.0",
    "open": "^10.1.2"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "typescript": "^5.7.3",
    "vite": "^6.3.5"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}